{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "app": {
        "background": {
            "scripts": [
                "js/main.js"
            ]
        }
    },
    "author": "Derek Bytheway",
    "description": "Turn Twitter's direct messages into a standalone web app",
    "icons": {
        "16": "img/icon_16.png",
        "128": "img/icon_128.png"
    },
    "manifest_version": 2,
    "minimum_chrome_version": "36.0",
    "name": "Chirp",
    "short_name": "Chirp",
    "version": "1.0",
    "permissions": [
        "webview",
        "https://twitter.com/"
    ]
}